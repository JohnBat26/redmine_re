if params[:re_building_block_multiple_values] == '1'
  page[:label_for_re_building_block_embedding_type_complete].remove_class_name "disabled_is_false"
  page[:label_for_re_building_block_embedding_type_complete].add_class_name "disabled_is_true"
  page[:re_building_block_embedding_type_complete][:disabled] = true 
  page << "if ($('re_building_block_embedding_type_complete').checked) {"
    page[:re_building_block_embedding_type_complete][:checked] = false

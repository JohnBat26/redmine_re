<div class="contextual">
  <% if artifact.id %>
    <%= watcher_tag(@artifact_properties, User.current) %>
    <%= link_to l(:button_delete), {
      :controller => 're_artifact_properties',
      :action => 'delete',
      :id => artifact.re_artifact_properties.id
    }, :class => 'icon icon-del' %>
  <% end %>
</div>
